<div class="skill-card item" data-itemid="{{_id}}">
    <div class="skill-info skill-name">
        <p><img id="icon" src="{{img}}" height = "24px">
        <a class="item-edit" >{{name}}</a></p>
    </div>
    <div class="skill-info" "skill-stats">
        <p> <input class="inline-edit" data-field="data.points" name="data.points" type="text" value = "{{data.points}}" placeholder="" data-dtype="Number"/>
        is spell? {{data.isSpell}}
        
        <a class="item-delete">
            <i class="fas fa-trash"></i>
        </a>
        </p>
    </div>
</div>